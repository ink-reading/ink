{
  "name": "ink",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
    "fmt": "prettier --plugin prettier-plugin-svelte --ignore-unknown --write .",
    "prepare": "husky",
    "db:gen": "drizzle-kie generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:check": "drizzle-kit check:pg",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio"
  },
  "lint-staged": {
    "**/*": "prettier --plugin prettier-plugin-svelte --ignore-unknown --write ."
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "5.0.0-svelte5.2",
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.22.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "pg": "^8.11.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "svelte": "5.0.0-next.148",
    "svelte-check": "^3.8.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "dayjs": "^1.11.11",
    "drizzle-orm": "^0.31.0",
    "lucia": "^2.7.7",
    "lucide-svelte": "^0.381.0",
    "postgres": "^3.4.4"
  }
}
