{
  "name": "ink",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
    "fmt": "prettier --plugin prettier-plugin-svelte --ignore-unknown --write .",
    "prepare": "husky install",
    "db:gen": "drizzle-kie generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:check": "drizzle-kit check:pg",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio"
  },
  "lint-staged": {
    "**/*": "prettier --plugin prettier-plugin-svelte --ignore-unknown --write ."
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "svelte": "^4.2.5",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "@prisma/client": "^5.6.0",
    "drizzle-orm": "^0.29.0",
    "lucia": "^2.7.4",
    "lucide-svelte": "^0.292.0",
    "postgres": "^3.4.3",
    "prisma": "^5.6.0"
  }
}
